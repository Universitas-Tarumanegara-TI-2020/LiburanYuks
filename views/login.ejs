<div class="container-md mt-3">
  <div class="card">
    <div class="card-body">
      <%- include ("partials/messages"); %>
      <form class="container-xl" action="/users/login" method="POST">
        <img src="/Gambar/login_icon.png" class="rounded mx-auto d-block" alt="" height="150px" style="padding-bottom: 10px;">
        <div class="mb-3 text-center">
          <label for="email" class="form-label ">Email address</label>
          <input type="email" id="email" name="email" class="form-control rounded-pill" placeholder="Enter Email"/>
        </div>
        <div class="mb-3 text-center">
          <label for="password" class="form-label">Password</label>
          <input type="password" id="password" name="password" class="form-control rounded-pill" placeholder="Enter Password"/>
        </div>
        <button type="submit" class="btn btn-outline-success rounded mx-auto d-block rounded-3">Log in</button>
      </form>
        No Account? <a href="/users/register">Register</a>
      </p>
    </div>
  </div>
</div>
